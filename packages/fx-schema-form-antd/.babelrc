{
    "presets": [
        "es2015"
    ],
    "env": {
        "plugins": [
            "transform-react-jsx",
            "transform-runtime",
            [
                "import",
                {
                    "libraryName": "antd",
                    "style": true
                }
            ]
        ],
        "production": {
            "plugins": [
                "add-module-exports"
                // "transform-react-jsx",
                // "transform-runtime"
            ]
        },
        "comments": false
    }
}